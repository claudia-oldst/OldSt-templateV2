{
    "name": "old-st-template",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "prepare": "husky install",
        "start": "nx serve",
        "build": "nx build",
        "test": "nx test",
        "web-app": "npx env-cmd -f .env.local nx serve web-app --skip-nx-cache",
        "web-app:staging": "npx env-cmd -f .env.staging nx serve web-app",
        "all": "npx nx run-many --target=serve --all --maxParallel=100",
        "prisma-local:format": "dotenv -e .env.local -- npx prisma format",
        "prisma-local:migrate": "dotenv -e .env.local -- npx prisma migrate dev --name init",
        "prisma-local:db-seed": "dotenv -e .env.local -- npx prisma db seed",
        "prisma-local:db-pull": "dotenv -e .env.local -- npx prisma db pull",
        "prisma-local:apply": "npm run prisma-local:format && npm run prisma-local:migrate",
        "build-web-app": "nx build web-app --skip-nx-cache",
        "build-authentication-service": "nx build authentication-service --skip-nx-cache",
        "build-cognito-custom-message": "nx build cognito-custom-message --skip-nx-cache",
        "build-admin-service": "nx build admin-service --skip-nx-cache --configuration=lambda-serverless",
        "build-user-service": "nx build user-service --skip-nx-cache --configuration=lambda-serverless",
        "build-logging-service": "nx build logging-service --skip-nx-cache",
        "build-file-service": "nx build file-service --skip-nx-cache --configuration=lambda-serverless",
        "build-email-service": "nx build email-service --skip-nx-cache --configuration=lambda-serverless",
        "storybook": "export NODE_OPTIONS=--openssl-legacy-provider && nx storybook ui",
        "list-apps-dirs": "find apps -mindepth 1 -maxdepth 1 -type d ! -name 'web-app' | awk -F/ '{print $NF}' > apps.txt"    

    },
